<% include partials/header2%>

<div class="container">
  
    <h1 style="text-align:center">Update Profile</h1>
       <% include partials/messages %>
      <div style="width:30%; margin:30px auto">
<form action="/update" method="POST">
    
    <div class="form-group">
        <input class="form-control" type="text" name="name" placeholder="name"  required>
      

    <div class="form-group">
         <input class="form-control" type="text" name="age"  placeholder="age" required>
         
     </div>
     
    <div class="form-group">
         <input class="form-control" type="text" name="height"  placeholder="height(in cm)" required>
         
     </div>
      <div class="form-group">
         <input class="form-control" type="text" name="zip"  placeholder="zip" required>
         
     </div>
     
    <button class="btn btn-md btn-default btn-block">
        Create
    </button>
    
</form>

<a class="container" href="/userprofiles"> Go Back</a>
</div>
</div>
